<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="at.footballapp.captainandroid.footballapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".pkgGUI.StatisticActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".pkgGUI.RemovePlayerActivity" />
        <activity android:name=".pkgGUI.RemoveMatchActivity" />
        <activity android:name=".pkgGUI.UpdateMatchActivity" />
        <activity android:name=".pkgGUI.EditTeamActivity" />
        <activity android:name=".pkgGUI.MainActivity" />
        <activity android:name=".pkgGUI.LoginActivity" />
        <activity android:name=".pkgGUI.MatchActivity" />
        <activity android:name=".pkgGUI.ProfileActivity">
        </activity>
    </application>

</manifest>